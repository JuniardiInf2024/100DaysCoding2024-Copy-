package days38;

public class Days38 {
    
    public static void main(String[] args) {
        // Pengisian nilai
        int a = 15; 
        System.out.println("Nilai awal adalah " +a);
        
        // Pengisian perkalian
        a = a * 5;
        System.out.println("Nilai a setelah perkalian 5: " + a);
        
        // Pengisian pembagian
        a = a / 3; 
        System.out.println("Nilai a setelah pembagian 3: " + a);
    }
}
